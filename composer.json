{
    "name": "jcergolj/base",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "intervention/image": "^2.5",
        "laravel/ui": "^2.0",
        "livewire/livewire": "^1.0",
        "matt-allan/laravel-code-style": "^0.5.1",
        "predis/predis": "^1.1",
        "protonemedia/laravel-verify-new-email": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "brainmaestro/composer-git-hooks": "^2.8",
        "brianium/paratest": "^4.0",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "jasonmccreary/laravel-test-assertions": "^0.5.0",
        "johnkary/phpunit-speedtrap": "^3.2",
        "laravel-shift/blueprint": "^1.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1.3",
        "nunomaduro/phpinsights": "^1.14",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "hooks": {
            "pre-commit": [
                "STAGED_FILES=$(git diff --name-only --cached)",
                "vendor/bin/php-cs-fixer fix",
                "for FILE in $STAGED_FILES ; do git add $FILE; done",
                "php artisan insights --no-interaction --min-quality=90 --min-complexity=90 --min-architecture=90 --min-style=95"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": ["app/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": ["tests/helpers.php"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "cghooks": "vendor/bin/cghooks"
    }
}
